{
  "name": "react-universal-live-maps",
  "description": "Livemaps for Tour of Norway",
  "author": "Gerhard Sletten (http://github.com/gerhardsletten)",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/gerhardsletten/react-universal-live-maps"
  },
  "homepage": "https://github.com/gerhardsletten/react-universal-live-maps",
  "keywords": [],
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "__DEVELOPMENT__",
      "webpackIsomorphicTools",
      "__DISABLE_SSR__",
      "__DEVTOOLS__",
      "__CLIENT__",
      "__SERVER__"
    ]
  },
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "lint": "eslint -c .eslintrc src api",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "test": "karma start",
    "setup": "npm install && node setup.js",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "body-parser": "^1.14.1",
    "clean-webpack-plugin": "^0.1.6",
    "compression": "^1.6.0",
    "concat-stream": "^1.5.1",
    "concurrently": "^0.1.1",
    "connect-mongo": "^1.1.0",
    "core-js": "2.3.0",
    "css-loader": "^0.23.1",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "hoist-non-react-statics": "^1.0.3",
    "http-proxy": "^1.12.0",
    "invariant": "^2.2.0",
    "jsdom": "^9.0.0",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^6.2.0",
    "lru-memoize": "^1.0.0",
    "map-props": "^1.0.0",
    "moment": "^2.13.0",
    "mongoose": "^4.4.14",
    "mongoose-geojson-schema": "^2.1.0",
    "object.omit": "^2.0.0",
    "piping": "^0.3.0",
    "postcss-loader": "^0.8.2",
    "pretty-error": "^1.2.0",
    "react": "^0.14.8",
    "react-datetime": "^2.1.0",
    "react-dom": "^0.14.1",
    "react-google-maps": "^4.10.1",
    "react-helmet": "^2.2.0",
    "react-redux": "^4.0.0",
    "react-router": "2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-async-connect": "^1.0.0-rc2",
    "redux-thunk": "^2.0.1",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "shortid": "^2.2.6",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "superagent": "^1.4.0",
    "togeojson": "^0.13.0",
    "turf": "^2.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-isomorphic-tools": "^2.2.18"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-typecheck": "^3.6.0",
    "better-npm-run": "0.0.8",
    "eslint": "1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.5.0",
    "eslint-plugin-standard": "^1.3.1",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-logger": "^2.6.1",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.0"
  },
  "engines": {
    "node": "5.10.1"
  }
}
